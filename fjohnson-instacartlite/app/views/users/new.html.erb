<div id="clearance" class="sign-up">
  <h2><%= t(".title") %></h2>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="submit-field">
      <%= form.submit %>
    </div>

    <div class="other-links">
      <h5><%= link_to 'Already have an account? Sign In.', sign_in_path %></h5>
    </div>
  <% end %>
</div>
